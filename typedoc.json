{
  "$schema": "https://typedoc.org/schema.json",
  "cleanOutputDir": true,
  "entryPoints": ["./src/**/*.ts"],
  "entryPointStrategy": "resolve",
  "exclude": ["./src/index.ts"],
  "includeVersion": true,
  "name": "Homebridge Shortcuts Buttons Plugin",
  "out": "typedoc",
  "readme": "none",
  "sort": ["kind", "source-order", "instance-first", "alphabetical"],
  "tsconfig": "./tsconfig.json",
  "visibilityFilters": {
    "external": true,
    "inherited": true,
    "private": true,
    "protected": true
  }
}
